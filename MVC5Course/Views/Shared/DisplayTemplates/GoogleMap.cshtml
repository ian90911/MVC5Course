@model Tuple<double?, double?>

@if (Model.Item1.HasValue && Model.Item2.HasValue)
{
    <iframe width="300"
            height="170"
            frameborder="0"
            scrolling="no"
            marginheight="0"
            marginwidth="0"
            src="https://maps.google.com/maps?q=@(Model.Item1.Value),@(Model.Item2.Value)&hl=es;z=14&amp;output=embed"></iframe>
}
